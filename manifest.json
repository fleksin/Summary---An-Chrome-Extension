{
  "name": "SummaryPopup",
  "description": "Move mouse over a link, it will show the summary.",
  "version": "1.0",  
   "permissions": [
    "tabs","http://*/*", "https://*/*", "file:///*"
  ],
 
 "content_scripts": [
  {
    "matches": ["http://*/*"],
    "js":["jquery-1.11.1.min.js","Content.js"],
	"css": ["style.css"]
  }
  ],
  
  "icons":{ "128" : "iconE.png"},
 
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "Hello.html"
}, 

  "manifest_version": 2
}

